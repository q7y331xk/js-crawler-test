<script type="text/javascript">
    
    (function() {
        var decHtml = function( str ){
          var dom = document.createElement('div');
          for(var i=0;i<3;i++){
            str = str.replace(/\&quot\;/gi,'\\\"');
            dom.innerHTML= str;
            str = dom.innerText;
          }
