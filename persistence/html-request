    "initComplete" : function(beacon, sundry, searchbar) {
        if (window.detailLinks && window.languageMaps) {
            var J = window.SCHeader.J;
        	//searchbar.setValidateItems([]);
        	J(window.SCHeader).on('languageChange',function(ev, params){
        	    ev.preventDefault();
        	    params = params || {};
        	    if(typeof(params.stopCall) == 'function'){
        	        params.stopCall();
        	    }
        	    var targetLang = languageMaps[params.targetLocale],
        	        targetLink = detailLinks[targetLang];
        	   // window.location.reload();
        	   // console.log(targetLink);
                window.location.href = targetLink;
        	});
        }
        searchbar.cem.subscribe('searchTypeChangeValue',function(e,args){
            searchbar.removeField('selectedTab');
            searchbar.addField('selectedTab',args.type);
        });
    }
};
